{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#c386f1",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "postfix": " ",
            "display_host": false
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#ff479c",
          "properties": {
            "prefix": "   ",
            "home_icon": "~",
            "folder_separator_icon": " \uE0b1 ",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#193549",
          "background": "#fffb38",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "local_changes_color": "#ff9248",
            "ahead_and_behind_color": "#f26d50",
            "behind_color": "#f17c37",
            "ahead_color": "#89d1dc",
            "stash_count_icon": "\uF692 "
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\uE0B4 ",
          "leading_diamond": "\uE0B6",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "properties": {
            "prefix": " \uF898 ",
            "display_version": true
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "\uE0B4 ",
          "leading_diamond": "\uE0B6",
          "foreground": "#111111",
          "background": "#FFDE57",
          "properties": {
            "prefix": " \uE235 ",
            "display_version": true,
            "display_mode": "files",
            "display_virtual_env": false
          }
        },
        {
          "type": "php",
          "style": "diamond",
          "trailing_diamond": "\uE0B4 ",
          "leading_diamond": "\uE0B6",
          "foreground": "#ffffff",
          "background": "#8993be",
          "properties": {
            "prefix": " \uE73D ",
            "display_version": true
          }
        }
      ]
    }
  ],
  "final_space": true,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }}"
}
